    "babel-jest": "^25.5.1",
    "gh-pages": "^2.2.0",
    "jest": "^25.5.3",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
